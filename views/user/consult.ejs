        <script>
            const symptoms = [
                "Fever", "Cough", "Headache", 
                 "Sore Throat", "Loss of Taste or Smell", 
                "Diarrhea", "Chest Pain", "Difficulty Breathing", 
                "Weakness", "Abdominal Pain", "Joint Pain", "Loss of Appetite", "Rash",
                "Vomiting","Dry Cough",
                "Frequent Urination", "Back Pain", "Constipation", "Sweating", "Heartburn",
                "Bloating", "Weight Loss", "Insomnia", "Anxiety", "Depression", "Memory Loss", "Runny Nose",
                
            ];
        
            const symptomButtons = document.getElementById('symptomButtons');
        
            symptoms.forEach(symptom => {
                const button = document.createElement('button');
                button.classList.add('symptom-button');
                button.textContent = symptom;
                button.addEventListener('click', () => {
                    button.classList.toggle('selected');
                });
                symptomButtons.appendChild(button);
            });
        </script>
        
